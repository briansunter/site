<ul class="flex justify-center">

    {% for icon, link in site.social_links %}

    <li class="mr-4">

        <a href="{{ link.link | safe }}" class="block text-gray-500 hover:text-pink-500" aria-label="{{ icon | capitalize }}">
            <img class="contact-icon w-10 h-10" src="{{link.icon}}">
        </a>

    </li>

    {% endfor %}

</ul>
